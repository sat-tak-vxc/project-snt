<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <type>creator</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>DeliveryAddressCategory__c</field>
        <field>DeliveryLandload__c</field>
        <field>DeliveryContact__c</field>
        <field>DeliveryAccount__c</field>
        <field>DeliveryAddressCharge__c</field>
        <field>DeliveryAddress__c</field>
        <field>SendMethod__c</field>
        <field>SendingRequestDate__c</field>
        <field>Seal__c</field>
        <field>delAfterSignCorrespondence__c</field>
        <field>Owner</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <type>adhoc</type>
            </approver>
        </assignedApprover>
        <label>承認ステップ</label>
        <name>SealApplicationApprovalStep</name>
    </approvalStep>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <finalApprovalActions>
        <action>
            <name>SealApplicationApproval</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>SealApplicationSecondaryCheckDateRegistr</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>SealApplicationSecondaryCheckerRegistrat</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>SealApplicationRejected</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>SealApplicationApplying</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>SealApplicationDateRegistration</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>SealApplicatRegistration</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>捺印申請承認プロセス</label>
    <processOrder>1</processOrder>
    <recallActions>
        <action>
            <name>ActionFieldUpdate</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOnly</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
