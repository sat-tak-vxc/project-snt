    <assignments>
        <name>Billing_ProcessManagement</name>
        <label>請求に工程管理を紐づける</label>
        <locationX>50</locationX>
        <locationY>695</locationY>
        <assignmentItems>
            <assignToReference>$Record.ProcessManagement__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>getProcessManagement.Id</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Update_Billing</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Billing_Check</name>
        <label>Billing_Check</label>
        <locationX>424</locationX>
        <locationY>335</locationY>
        <defaultConnector>
            <targetReference>Update_Billing</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>デフォルトの結果</defaultConnectorLabel>
        <rules>
            <name>AA01</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.OperationCategory__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>契約金</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>getProcessManagement</targetReference>
            </connector>
            <label>業務区分が契約金</label>
        </rules>
        <rules>
            <name>AA02</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.OperationCategory__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>引越</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.OperationCategory__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>手数料</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.OperationCategory__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>その他</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.OperationCategory__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>家賃</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.OperationCategory__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>委託料</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Billing</targetReference>
            </connector>
            <label>業務区分が引越、手数料、その他、家賃、委託料</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_Mang</name>
        <label>管理外かチェック</label>
        <locationX>182</locationX>
        <locationY>575</locationY>
        <defaultConnector>
            <targetReference>Update_Billing</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>デフォルトの結果</defaultConnectorLabel>
        <rules>
            <name>Stasus_NoMang</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>getProcessManagement.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>管理外</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Billing_ProcessManagement</targetReference>
            </connector>
            <label>ステータスが管理外</label>
        </rules>
    </decisions>
