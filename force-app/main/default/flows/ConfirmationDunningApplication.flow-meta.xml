    <decisions>
        <name>decision_IsSendSuccess</name>
        <label>送信成功判定</label>
        <locationX>681</locationX>
        <locationY>729</locationY>
        <defaultConnectorLabel>送信失敗</defaultConnectorLabel>
        <rules>
            <name>isSendSuccess</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_IsSendSuccess</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>update_Cancel</targetReference>
            </connector>
            <label>送信成功</label>
        </rules>
    </decisions>
