    <decisions>
        <name>decision_PrePaymentStart</name>
        <label>前納期間と支払期間の比較</label>
        <locationX>1291</locationX>
        <locationY>846</locationY>
        <defaultConnector>
            <targetReference>assign_emptyVariable</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>デフォルトの結果</defaultConnectorLabel>
        <rules>
            <name>isAllConnotation</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>formula_PaymentEndRentPlusOne</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>get_GargeCertification.PrepaymentEndDate__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>get_GargeCertification.ParkingFeePaymentStartDate__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>formula_PaymentStartRentPlusOne</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>assign_Rent</targetReference>
            </connector>
            <label>支払開始＜前納開始（当月から）＜前納終了＜支払終了</label>
        </rules>
        <rules>
            <name>isFirstHalfConnotation</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>get_GargeCertification.PrepaymentEndDate__c</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>formula_PaymentEndRentPlusOne</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>formula_PaymentEndRentPlusOne</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>get_GargeCertification.ParkingFeePaymentStartDate__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>get_GargeCertification.ParkingFeePaymentStartDate__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>formula_PaymentStartRentPlusOne</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>assign_Rent2</targetReference>
            </connector>
            <label>支払開始＜前納開始（当月から）＜支払終了＜＝前納終了</label>
        </rules>
        <rules>
            <name>isLatterHalfConnotation</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>formula_PaymentEndRentPlusOne</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>get_GargeCertification.PrepaymentEndDate__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>get_GargeCertification.PrepaymentEndDate__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>formula_PaymentStartRentPlusOne</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>formula_PaymentStartRentPlusOne</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>get_GargeCertification.ParkingFeePaymentStartDate__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>assign_Rent1</targetReference>
            </connector>
            <label>前納開始（当月から）＜＝支払開始＜前納終了＜支払終了</label>
        </rules>
        <rules>
            <name>isEncase</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>get_GargeCertification.PrepaymentEndDate__c</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>formula_PaymentEndRentPlusOne</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>formula_PaymentStartRentPlusOne</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
