    <formulas>
        <name>formula_Conversion</name>
        <dataType>Number</dataType>
        <expression>VALUE(TEXT({!var_InputCancelCalculate.ServiceContractCancel__r.DepositRefundDate__c}))</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>formula_Date</name>
        <dataType>Date</dataType>
        <expression>IF(
MONTH( {!var_InputCompletionDate}) = 12,
DATE(  YEAR( {!var_InputCompletionDate} ) +1, 1, {!formula_Conversion}),
DATE(  YEAR( {!var_InputCompletionDate} ), MONTH( {!var_InputCompletionDate}) + 1, {!formula_Conversion})
)</expression>
    </formulas>
    <formulas>
        <name>formula_EndOfMonth</name>
        <dataType>Date</dataType>
        <expression>IF ( OR(MONTH( {!var_InputCompletionDate} ) = 12,MONTH( {!var_InputCompletionDate} ) = 11),
DATE( YEAR( {!var_InputCompletionDate} ), 12, 31 ) ,
DATE( YEAR( {!var_InputCompletionDate} ), MONTH( {!var_InputCompletionDate} ) + 2, 1 ) - 1)</expression>
    </formulas>
    <formulas>
        <name>formula_EndOfMonth1</name>
        <dataType>Date</dataType>
        <expression>DATE( CASE( MONTH(  {!var_InputCompletionDate}) , 12, YEAR(  {!var_InputCompletionDate}) + 1, YEAR(  {!var_InputCompletionDate}) ) ,CASE( MONTH(  {!var_InputCompletionDate}) , 12, 2, MONTH(  {!var_InputCompletionDate}) + 2 ) ,1 ) - 1</expression>
    </formulas>
    <formulas>
        <name>formula_NextMonthFirstDate</name>
        <dataType>Date</dataType>
        <expression>IF(
MONTH( {!var_InputCompletionDate}) = 12,
DATE(  YEAR( {!var_InputCompletionDate} ) +1, 2, {!formula_Conversion}),
IF(MONTH( {!var_InputCompletionDate}) = 11,
DATE(  YEAR( {!var_InputCompletionDate} ) +1, 1, {!formula_Conversion}),
DATE(  YEAR( {!var_InputCompletionDate} ), MONTH( {!var_InputCompletionDate}) + 2, {!formula_Conversion})
))</expression>
    </formulas>
