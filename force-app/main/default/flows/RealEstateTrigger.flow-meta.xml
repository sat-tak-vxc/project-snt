<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>54.0</apiVersion>
    <decisions>
        <name>decision_RealEstate</name>
        <label>物件レコードの確認</label>
        <locationX>182</locationX>
        <locationY>575</locationY>
        <defaultConnector>
            <targetReference>isChangedResident</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>デフォルトの結果</defaultConnectorLabel>
        <rules>
            <name>decision_NewOrUpdateContractEndDate</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>formula_isnew</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.ContractEndDate__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>frow_RenewalRecordAutoCreateDateCreate</targetReference>
            </connector>
            <label>新規作成または契約終期更新時</label>
        </rules>
    </decisions>
    <decisions>
        <name>isChangedResident</name>
        <label>入居者変更確認</label>
        <locationX>182</locationX>
        <locationY>911</locationY>
        <defaultConnectorLabel>デフォルトの結果</defaultConnectorLabel>
        <rules>
            <name>ChangedResident</name>
            <conditionLogic>1 AND (2 OR 3)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.ContractorName__c</leftValueReference>
                <operator>IsChanged</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.ContractorName__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Account__r.EmptyStaff__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.ContractorName__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CreateResidentHistory</targetReference>
            </connector>
            <label>入居者変更</label>
        </rules>
    </decisions>
    <description>起動条件を変更</description>
    <environments>Default</environments>
    <formulas>
        <name>formula_isnew</name>
        <dataType>Boolean</dataType>
        <expression>isnew()</expression>
    </formulas>
    <interviewLabel>物件トリガー {!$Flow.CurrentDateTime}</interviewLabel>
    <label>RE_物件トリガー</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>update_RealEstateOwner</name>
        <label>物件の所有者変更</label>
        <locationX>182</locationX>
        <locationY>335</locationY>
        <connector>
            <targetReference>CreateShareObjectRealEstate</targetReference>
        </connector>
        <inputAssignments>
            <field>OwnerId</field>
            <value>
                <elementReference>$Setup.SyatakusanSetting__c.SystemUserId__c</elementReference>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>56</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>update_RealEstateOwner</targetReference>
        </connector>
        <filterFormula>OR(ISNEW(),ISCHANGED({!$Record.BrrowerCancelNoticeNum__c}),ISCHANGED({!$Record.BrrowerCancelNoticeUnit__c}),ISCHANGED({!$Record.RenewalCategory__c}),ISCHANGED({!$Record.RenewalApprovalCategory__c}))</filterFormula>
        <object>RealEstate__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Draft</status>
    <subflows>
        <name>CreateResidentHistory</name>
        <label>入居者変更履歴作成</label>
        <locationX>50</locationX>
        <locationY>1031</locationY>
        <flowName>CreateResidentHistory</flowName>
        <inputAssignments>
            <name>recordId</name>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </inputAssignments>
    </subflows>
    <subflows>
        <name>CreateShareObjectRealEstate</name>
        <label>RE_シェアオブジェクト作成（物件）</label>
        <locationX>182</locationX>
        <locationY>455</locationY>
        <connector>
            <targetReference>decision_RealEstate</targetReference>
        </connector>
        <flowName>CreateShareObjectRealEstate</flowName>
        <inputAssignments>
            <name>RecordId</name>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </inputAssignments>
    </subflows>
    <subflows>
        <name>frow_RenewalRecordAutoCreateDateCreate</name>
        <label>RE_更新レコード自動作成日の更新</label>
        <locationX>50</locationX>
        <locationY>695</locationY>
        <connector>
            <targetReference>isChangedResident</targetReference>
        </connector>
        <flowName>RenewalRecordAutoCreateDateCreate</flowName>
        <inputAssignments>
            <name>var_RealEstateRecord</name>
            <value>
                <elementReference>$Record</elementReference>
            </value>
        </inputAssignments>
    </subflows>
</Flow>
