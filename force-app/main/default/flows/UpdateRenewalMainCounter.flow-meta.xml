<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>55.0</apiVersion>
    <description>「連絡注意事項」に取引先「備考」を追加</description>
    <environments>Default</environments>
    <formulas>
        <name>formula_IsProxyContactAutoNotice</name>
        <dataType>Boolean</dataType>
        <expression>IF (ISBLANK({!get_Renewal.RenewalCounter__c}), false, {!get_Account.IsAutoNotice__c})</expression>
    </formulas>
    <interviewLabel>RE_更新窓口更新 {!$Flow.CurrentDateTime}</interviewLabel>
    <label>RE_更新窓口更新</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>FREE_FORM_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <name>get_Account</name>
        <label>取引先レコード取得</label>
        <locationX>619</locationX>
        <locationY>347</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>update_Renewal</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>get_Renewal.RenewalCounter__r.Account__c</elementReference>
            </value>
        </filters>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>Account</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <recordLookups>
        <name>get_Renewal</name>
        <label>更新レコードの取得</label>
        <locationX>617</locationX>
        <locationY>209</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>get_Account</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>recordId</elementReference>
            </value>
        </filters>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>Renewal__c</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <recordUpdates>
        <name>update_Renewal</name>
        <label>更新レコード更新</label>
        <locationX>617</locationX>
        <locationY>495</locationY>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>get_Renewal.Id</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>ContactNotes__c</field>
            <value>
                <elementReference>get_Account.CompanywideNote__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>IsRenewalCounterAutoNotice__c</field>
            <value>
                <elementReference>formula_IsProxyContactAutoNotice</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>RenewalMainCounterEmail__c</field>
            <value>
                <elementReference>get_Account.MainEmail__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>RenewalMainCounterFAX__c</field>
            <value>
                <elementReference>get_Account.Fax</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>RenewalMainCounterPhone__c</field>
            <value>
                <elementReference>get_Account.Phone</elementReference>
            </value>
        </inputAssignments>
        <object>Renewal__c</object>
    </recordUpdates>
    <runInMode>SystemModeWithoutSharing</runInMode>
    <start>
        <locationX>490</locationX>
        <locationY>48</locationY>
        <connector>
            <targetReference>get_Renewal</targetReference>
        </connector>
    </start>
    <status>Active</status>
    <variables>
        <name>recordId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>false</isOutput>
    </variables>
    <variables>
        <name>var_AccountId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>false</isOutput>
    </variables>
</Flow>
