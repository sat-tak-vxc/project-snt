        <outputAssignments>
            <assignToReference>var_isFaxFlow</assignToReference>
            <name>var_isFaxFlow</name>
        </outputAssignments>
        <outputAssignments>
            <assignToReference>var_IsSendSuccess</assignToReference>
            <name>var_IsSendSuccess</name>
        </outputAssignments>
