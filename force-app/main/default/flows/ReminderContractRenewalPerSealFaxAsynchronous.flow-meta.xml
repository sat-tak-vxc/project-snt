        </inputAssignments>
        <inputAssignments>
            <name>var_InputReportName</name>
            <value>
                <elementReference>$Setup.ReportNameSetting__c.ReturnReminderContractRenewal__c</elementReference>
            </value>
        </inputAssignments>
        <outputAssignments>
