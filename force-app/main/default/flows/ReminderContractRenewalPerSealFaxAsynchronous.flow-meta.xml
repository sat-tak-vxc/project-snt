<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>54.0</apiVersion>
    <assignments>
        <name>assign_StandBy</name>
        <label>保管督促待機フラグ割り当て</label>
        <locationX>846</locationX>
        <locationY>888</locationY>
        <assignmentItems>
            <assignToReference>var_SealRecord.IsStorageReminderStandBy__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>var_SealRecord.Id</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>update_Seal</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>decision_IsSendSuccess</name>
        <label>送信成功判定</label>
        <locationX>957</locationX>
        <locationY>601</locationY>
        <defaultConnectorLabel>デフォルトの結果</defaultConnectorLabel>
        <rules>
            <name>IsSendSuccess</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_IsSendSuccess</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>update_Renewal</targetReference>
            </connector>
            <label>送信成功</label>
        </rules>
        <rules>
            <name>isFaxFlow</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>var_isFaxFlow</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <label>faxFlowに飛ばす</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>formula_DaysLater</name>
        <dataType>Date</dataType>
        <expression>TODAY()+{!$Record.Renewal__r.RenewalCounter__r.Account__r.ServiceContractRenewalConditionChange__r.ContractStorageReminderAddDays__c}</expression>
    </formulas>
    <interviewLabel>RE_捺印管理レコードごとの締結書類の督促（Fax）(非同期) {!$Flow.CurrentDateTime}</interviewLabel>
    <label>RE_捺印管理レコードごとの締結書類の督促（Fax）(非同期)</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>FREE_FORM_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>update_Renewal</name>
        <label>書類レコードを更新</label>
        <locationX>841</locationX>
        <locationY>764</locationY>
        <connector>
            <targetReference>assign_StandBy</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>IsStorageReminderTarget__c</field>
            <operator>EqualTo</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Seal__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>CollectScheduledDate__c</field>
            <value>
                <elementReference>formula_DaysLater</elementReference>
            </value>
        </inputAssignments>
        <object>Document__c</object>
    </recordUpdates>
    <recordUpdates>
        <name>update_Seal</name>
        <label>捺印管理レコードを更新</label>
        <locationX>843</locationX>
        <locationY>1040</locationY>
        <inputReference>var_SealRecord</inputReference>
    </recordUpdates>
    <start>
        <locationX>755</locationX>
        <locationY>47</locationY>
        <filterLogic>and</filterLogic>
        <filters>
            <field>isFaxFlow__c</field>
            <operator>IsChanged</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Renewal__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <object>Seal__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <scheduledPaths>
            <connector>
                <targetReference>RM_ReportOutput</targetReference>
            </connector>
            <pathType>AsyncAfterCommit</pathType>
        </scheduledPaths>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
    <subflows>
